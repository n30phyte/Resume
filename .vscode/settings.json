{
  "latex-workshop.latex.recipes": [
    {
      "name": "Build resume",
      "tools": [
        "Build-win"
      ]
    },
    {
      "name": "Clean resume",
      "tools": [
        "Clean"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "Build",
      "command": "make"
    },
    {
      "name": "Clean",
      "command": "make clean"
    },
    {
      "name": "Build-win",
      "command": "latexmk -synctex=1 -lualatex -file-line-error -interaction=nonstopmode -halt-on-error -outdir=build resume.tex"
    }
  ]
}
